programa { 
  
     cadeia nome = "Coach" 
  
     inteiro idade = 17  
  
     cadeia cidade = "Rio Negro" 
  
     cadeia estado = "PR " 
  
  
  
   funcao inicio() { 
        
  
  
  
  
  
  
  
  
  
  
  
         escreva("Meu nome é ", nome) 
         escreva("\nTenho ", idade) 
         escreva(" moro na cidade de ", cidade) 
         escreva(" que fica no estado de ", estado) 
          
         
  
   } 
  
 }
